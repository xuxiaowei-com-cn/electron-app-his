{
  "name": "electron-app-his",
  "productName": "electron-app-his",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "set NODE_ENV=development&& chcp 65001 && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --color"
  },
  "keywords": [],
  "author": {
    "name": "徐晓伟",
    "email": "xuxiaowei@xuxiaowei.com.cn"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/assets/img/icon.ico",
        "asar": true
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_app_his",
            "setupIcon": "src/assets/img/icon.ico"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.56",
    "@electron-forge/maker-deb": "6.0.0-beta.56",
    "@electron-forge/maker-rpm": "6.0.0-beta.56",
    "@electron-forge/maker-squirrel": "6.0.0-beta.56",
    "@electron-forge/maker-zip": "6.0.0-beta.56",
    "@electron-forge/plugin-webpack": "6.0.0-beta.56",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.3.0",
    "electron": "v22.3.25",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.0",
    "node-loader": "^2.0.0",
    "node-sass": "^6.0.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14"
  },
  "dependencies": {
    "crypto-js": "^4.0.0",
    "electron-log": "^4.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^6.0.0",
    "element-ui": "^2.13.2",
    "jsencrypt": "^3.0.0-rc.1",
    "vue": "^2.6.12",
    "vue-router": "^3.4.5",
    "vuex": "^3.5.1"
  }
}
